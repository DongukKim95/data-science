   "source": [
    "def solution(brown, yellow):\n",
    "    s = brown + yellow\n",
    "\n",
    "    for n in range(1, s//2):\n",
    "        if s % n == 0 and n >= s/n:\n",
    "            if (n-2)*(int(s/n)-2) == yellow:\n",
    "                return [n, int(s/n)]\n",
    "\n",
    "    return []"
   ]
